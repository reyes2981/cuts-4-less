<h1> Schedule an Appointment </h1>

<%= form_for(@appointment) do |f| %>
    <%= f.label :hairdresser_id, "Hair Stylist" %>
    <%= f.text_field :hairdresser_id %>
    <p>
    <%= f.label :datetime %>
    <%= f.datetime_field :datetime %>
    <p>
    <%= f.submit "Schedule" %>

<%end%>

